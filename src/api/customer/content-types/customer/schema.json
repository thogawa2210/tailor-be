{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::address.address",
      "inversedBy": "customers"
    },
    "description": {
      "type": "text"
    },
    "email": {
      "type": "email"
    },
    "dob": {
      "type": "date"
    },
    "measurement": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::measurement.measurement",
      "mappedBy": "customer"
    }
  }
}
