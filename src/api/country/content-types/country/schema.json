{
  "kind": "collectionType",
  "collectionName": "countries",
  "info": {
    "singularName": "country",
    "pluralName": "countries",
    "displayName": "Country"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "cities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::city.city",
      "mappedBy": "country_id"
    },
    "fabric_brands": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::fabric-brand.fabric-brand",
      "mappedBy": "origin_country"
    }
  }
}
